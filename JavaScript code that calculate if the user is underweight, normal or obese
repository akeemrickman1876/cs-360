let height = 0;
let BMI = 0;
let weight = 0;


function getHeight () {
console.log("Enter your height: ");
}

function setHeight(newHeight){
height  =  newHeight;
}

function getWeight () {
console.log("Enter your weight: ");
}

function setWeight(newWeight){
weight  =  newWeight;
}

BMI = (708*weight)/(height*height);

console.log (BMI)

function calculateMetricBMI(weight, height)
{
  let BMI = (weight * height * height);
  return BMI;
  
}
if (BMI<18.5){
    return "You are underweight";}
    
else if (BMI >=18.5 && BMI <= 24.9){
    return "You are normal";}
    
else if (BMI >=25.0 && BMI <= 29.0){
    return "You are overweight";}
    
else if (BMI >=30.0){
    return "You are obese";}
